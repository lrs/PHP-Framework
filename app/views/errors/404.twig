{% set title = _('error.notfound.title') %}
{% set desc = _('error.notfound.description') %}

{% extends 'layouts/default.twig' %}

{% block banner %}
    {{ include('components/banner.twig', { title: title }) }}
{% endblock %}

{% block content %}
    <div class="grid__col-12">
        <p>{{ title }}!</p>
        <p>{{ desc }}</p>
    </div>
{% endblock %}
